{
  "name": "outfique",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --ext ts --exec tsx src/server.ts",
    "build": "pnpm run clean && pnpm run compile",
    "compile": "tsc --build tsconfig.json",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "build:prod": "pnpm run build",
    "clean": "rm -rf dist",
    "start": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "commit": "cz-customizable",
    "prepare": "husky"
  },
  "config": {
  "commitizen": {
    "path": "cz-customizable"
  }
},
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "npm run type-check"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mastra/core": "^0.18.0",
    "@mastra/libsql": "^0.14.3",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "adapters@latest": "link:better-auth/adapters@latest",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "better-auth": "^1.3.27",
    "chalk": "^4.1.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mastra": "^0.13.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "openai": "^5.22.1",
    "pino": "^9.11.0",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@prisma/client": "6.14.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "cz-customizable": "^7.5.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
